{
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "dist",
    "node_modules"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": [
        "./src/*",
      ],
    },
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    // "allowJs": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "lib": [
      "dom",
      "es2016"
    ],
    "resolveJsonModule": true
  }
}